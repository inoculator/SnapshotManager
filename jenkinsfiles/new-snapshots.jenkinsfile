pipeline {
    agent {label "DERDAPP006"}
    options {
        durabilityHint 'MAX_SURVIVABILITY'
        disableConcurrentBuilds()
        disableResume()
    }
    stages {
        stage ('getsnapshots') {
            steps {
                script {
                    pwsh '''
                        . ${env:WORKSPACE}/functions/new-snapshot.ps1
                        write-host "Start.....`n"
                        $result=new-snapshot -verbose
                        $result|out-string
                        write-host "....End"
                    '''
                }
            }
        }
    }
}