pipeline {
    agent {label "DERDAPP006"}
    options {
        durabilityHint 'MAX_SURVIVABILITY'
        disableConcurrentBuilds()
        disableResume()
    }
    stages {
        stage ('getsnapshots') {
            steps {
                script {
                    pwsh '''
                        . ${env:WORKSPACE}/functions/get-snapshot.ps1
                        get-snapshot
                    '''
                }
            }
        }
    }
}